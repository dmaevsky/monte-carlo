  $34 = tf.getBackend()
  $35 = tf.enableProdMode()
  naive = estimate_pi(m)
  x = tf.randomUniform([m])
  y = tf.randomUniform([m])
  $33 = tf.tidy(() => tf.mean(x*x + y*y < 1)*4).toString()
