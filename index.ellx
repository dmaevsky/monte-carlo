    {
      "node": "$34"
    },
    {
      "node": "$35"
    }
  ],
  [,,
    {
      "node": "naive",
      "expansion": {
        "vertical": true,
        "labelsLeft": true,
        "secondary": false,
        "height": 2,
        "width": 1
      }
    }
  ],,,
  [,
    {
      "node": "x"
    }
  ],
  [,
    {
      "node": "y"
    }
  ],,
  [,
    {
      "node": "$33"
    }
  ]
